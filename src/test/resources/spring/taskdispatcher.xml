<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.0.xsd"
        >

    <bean id="taskDispatcher" class="com.tyyd.framework.dat.taskdispatch.TaskDispatchFactoryBean" init-method="start">
        <property name="clusterName" value="TEST_CLUSTER"/>
        <property name="registryAddress" value="zookeeper://172.23.2.101:2181"/>
        <property name="configs">
            <props>
                <prop key="zk.client">zkclient</prop>
                <prop key="task.queue">mysql</prop>
                <prop key="jdbc.url">jdbc:mysql://localhost:3306/dat?useUnicode=true&amp;autoReconnect=true&amp;characterEncoding=utf-8&amp;useSSL=false&amp;serverTimezone=Hongkong</prop>
                <prop key="jdbc.username">root</prop>
                <prop key="jdbc.password">root</prop>
                <prop key="druid.validationQuery">select 1</prop>
            </props>
        </property>
    </bean>

</beans>